<div style="padding-left: 15px; padding-right: 15px;">
  <fieldset class="scheduler-border row">
    <legend class="scheduler-border">Descrição</legend>
    <div class="form-group input-group">
      <div class="input-group-prepend">
        <span class="input-group-text"><i class="fa fa-address-card-o"></i></span>
      </div>
      <input type="text" name="Descricao" class="form-control" placeholder="Descrição" required
        value="<%= model.Descricao %>">
    </div>
  </fieldset>
</div>

<!-- <div style="padding-left: 15px; padding-right: 15px;"> -->
<div class="row" style="margin-left: 0px; margin-right: 0px;">
  <div class="col-md-6">
    <fieldset class="scheduler-border">
      <legend class="scheduler-border">Horário de Monitoramento</legend>
      <div class="form-group row">
        <div class="input-group-prepend col-md-5">
          <span class="input-group-text"><i class="fa fa-clock-o"></i></span>
          <input type="time" name="HoraInicial" class="form-control" placeholder="Inicial"
            value="<%= model.HoraInicial %>" style="width: 100px;">
        </div>
        <div class="col-form-label col-md-1">
          a
        </div>
        <div class="input-group-prepend col-md-6"">
            <span class=" input-group-text"><i class="fa fa-clock-o"></i></span>
          <input type="time" name="HoraFinal" class="form-control" placeholder="Final" value="<%= model.HoraFinal %>"
            style="width: 100px;">
        </div>
      </div>
    </fieldset>
  </div>
  <div class="col-md-6">
    <fieldset class="scheduler-border">
      <legend class="scheduler-border">Tempo Jogo</legend>
      <div class="form-group row">
        <div class="input-group-prepend col-md-5">
          <span class="input-group-text"><i class="fa fa-clock-o"></i></span>
          <input type="number" name="TempoJogoInicial" class="form-control" placeholder="Inicial" min="0" max="90"
            step="1" value="<%= model.TempoJogoInicial %>" style="width: 100px;">
        </div>
        <div class="col-md-1">
          a
        </div>
        <div class="input-group-prepend col-md-6">
          <span class="input-group-text"><i class="fa fa-clock-o"></i></span>
          <input type="number" name="TempoJogoFinal" class="form-control" placeholder="Final" min="0" max="90" step="1"
            value="<%= model.TempoJogoFinal %>" style="width: 100px;">
        </div>
      </div>
    </fieldset>
  </div>
</div>

